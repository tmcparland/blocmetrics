 <div class="row">
   <div class="col-md-8">
     <div class="media">
       <div class="media-body">
         <h2 class="media-heading">Welcome <%= @user.email %>!</h2>
         <small>
         </small>
       </div>
     </div>
   </div>
 </div>
 
 <h3>Applications</h3>

<div class="row">
  <div class="col-md-8">
     <% @user.registered_applications.each do |user.registered_application| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
             <%= link_to registered_application.name, user.registered_application %>
           </h4>
           <small>
             <%= user.registered_application.url %>
           </small>
         </div>
       </div>
     <% end %>
   </div>