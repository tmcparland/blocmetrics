 <h1>Edit Application</h1>
 
 <div class="row">
   <div class="col-md-4">
     <p>Guidelines for Applications</p>
     <ul>
       <li>Nothing innappropriate.</li>
       <li>Puppy applications prefered.</li>
     </ul>
   </div>
   <div class="col-md-8">
     <%= form_for @registered_application do |f| %>
       <div class="form-group">
         <%= f.label :name %>
         <%= f.text_field :name, class: 'form-control', placeholder: "Enter application name" %>
       </div>
       <div class="form-group">
         <%= f.label :url %>
         <%= f.text_area :url, rows: 8, class: 'form-control', placeholder: "Enter URL for application" %>
       </div>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
   </div>
 </div>